      <BlogPost id="1078" key="f843c1dc-6dac-4620-a9e2-e52a003f462b" parentID="1075" level="3" creatorID="0" sortOrder="2" createDate="2015-12-03T21:06:56" updateDate="2015-12-03T21:31:32" nodeName="Only the curious have something to find" urlName="only-the-curious-have-something-to-find" path="-1,1063,1075,1078" isDoc="" nodeType="1058" creatorName="Craig" writerName="Craig" writerID="0" template="1055" nodeTypeAlias="BlogPost">
