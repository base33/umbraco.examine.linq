              "controls": [
                {
                  "value": {
                    "focalPoint": {
                      "left": 0.5,
                      "top": 0.5056179775280899
                    },
                    "id": 1123,
                    "image": "/media/1015/14257753719_2c02b94030_h.jpg",
                    "thumbnail": "/umbraco/backoffice/UmbracoApi/Images/GetBigThumbnail?originalImagePath=%2Fmedia%2F1015%2F14257753719_2c02b94030_h.jpg"
                  },
                  "editor": {
